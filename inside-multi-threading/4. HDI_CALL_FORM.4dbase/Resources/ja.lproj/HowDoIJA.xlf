      <group>
        <trans-unit resname="Demo...">
          <target>新しいフォームメッセージコマンド…</target>
        </trans-unit>
        <trans-unit resname="Quit 4D">
          <target>閉じる</target>
        </trans-unit>
        <trans-unit resname=" send ">
          <target>からのメッセージ: </target>
        </trans-unit>
